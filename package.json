{
  "name": "ember-cli-dependency-checker",
  "version": "3.3.1",
  "description": "Ember CLI addon for detecting missing npm and bower dependencies before executing ember commands",
  "keywords": [
    "bower",
    "check",
    "dependency",
    "ember",
    "ember-addon",
    "ember-cli",
    "node",
    "npm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/quaertym/ember-cli-dependency-checker.git"
  },
  "license": "MIT",
  "author": "Emre Unal",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/quaertym/ember-cli-dependency-checker/issues"
  },
  "scripts": {
    "lint:js": "eslint .",
    "test": "node tests/runner",
    "test:debug": "node debug tests/runner"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "find-yarn-workspace-root": "^1.2.1",
    "is-git-url": "^1.0.0",
    "resolve": "^1.22.0",
    "semver": "^5.7.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.6",
    "ember-cli": "~3.2.0",
    "ember-cli-3.4": "npm:ember-cli@~3.4.0",
    "ember-cli-3.8": "npm:ember-cli@~3.8.0",
    "ember-cli-3.12": "npm:ember-cli@~3.12.0",
    "ember-cli-3.16": "npm:ember-cli@~3.16.0",
    "ember-cli-3.20": "npm:ember-cli@~3.20.0",
    "ember-cli-3.24": "npm:ember-cli@~3.24.0",
    "ember-cli-3.28": "npm:ember-cli@~3.28.0",
    "ember-cli-beta": "npm:ember-cli@beta",
    "ember-cli-latest": "npm:ember-cli@latest",
    "eslint": "^7.32.0",
    "eslint-plugin-chai": "^0.0.1",
    "eslint-plugin-ember": "^10.6.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "mocha-eslint": "^7.0.0",
    "scenario-tester": "^2.1.1",
    "sinon": "^13.0.2"
  },
  "peerDependencies": {
    "ember-cli": "^3.2.0 || ^4.0.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "readmeFile": "README.md"
}
